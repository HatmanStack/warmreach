{
  "name": "warmreach-client",
  "version": "1.0.0",
  "description": "WarmReach client â€” Electron tray app with Puppeteer automation",
  "main": "src/server.js",
  "type": "module",
  "imports": {
    "#utils/*": "./src/shared/utils/*",
    "#shared-config/*": "./src/shared/config/*",
    "#config": "./config/index.js",
    "#routes/*": "./routes/*",
    "#schemas/*": "./schemas/*"
  },
  "scripts": {
    "start": "node --import tsx src/server.js",
    "dev": "nodemon --exec 'node --import tsx' src/server.js",
    "electron:dev": "electron --import tsx electron-main.js",
    "electron:build": "electron-builder",
    "build": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.8.3",
    "express": "^5.2.1",
    "ioredis": "^5.9.3",
    "jose": "^6.1.3",
    "libsodium-wrappers-sumo": "^0.8.2",
    "puppeteer": "^24.37.5",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tsx": "^4.19.2",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "linkedin",
    "automation",
    "puppeteer",
    "search",
    "scraping"
  ],
  "author": "Your Name",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "qs": "^6.14.1"
  }
}
